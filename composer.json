{
    "name": "hfg/video",
    "description": "Embed YouTube and Vimeo Videos",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "hfgvideo"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Niklas Sonnenschein",
            "homepage": "https://niklassonnenschein.de"
        }
    ],
    "require": {
        "craftcms/cms": "^3.3.0"
    },
    "autoload": {
        "psr-4": {
          "hfg\\hfgvideo\\": "src/"
        }
    },
    "extra": {
        "name": "HfG Video",
        "handle": "hfgvideo",
        "hasCpSettings": true,
        "hasCpSection": false,
        "class": "hfg\\hfgvideo\\Hfgvideo"
    }
}
